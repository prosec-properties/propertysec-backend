{
  "name": "properties-backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node build/bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --watch",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "migrate": "node ace migration:run",
    "seed": "echo 'seeding database' && node ace db:seed",
    "db": "echo 'running migration' && node ace migration:run && echo 'seeding database' && node ace db:seed"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js",
    "#types/*": "./types/*.js",
    "#constants/*": "./app/constants/*.js",
    "#helpers/*": [
      "./app/helpers/*.js"
    ],
    "#interfaces/*": "./app/interfaces/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.8.2",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/api-client": "^2.0.3",
    "@japa/assert": "^3.0.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.7.26",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.16.5",
    "@types/uuid": "^9.0.8",
    "eslint": "^8.57.1",
    "hot-hook": "^0.2.6",
    "pino-pretty": "^11.2.2",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@adonisjs/ally": "^5.0.2",
    "@adonisjs/auth": "^9.2.3",
    "@adonisjs/bouncer": "^3.1.3",
    "@adonisjs/core": "^6.14.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/limiter": "^2.3.2",
    "@adonisjs/lucid": "^20.6.0",
    "@adonisjs/mail": "^9.2.2",
    "@adonisjs/redis": "^9.1.0",
    "@azure/storage-blob": "^12.24.0",
    "@vinejs/vine": "^2.1.0",
    "axios": "^1.7.7",
    "bullmq": "^5.13.2",
    "device-detector-js": "^3.0.3",
    "edge.js": "^6.2.0",
    "jsonwebtoken": "^9.0.2",
    "luxon": "^3.5.0",
    "mjml": "^4.15.3",
    "mysql2": "^3.11.3",
    "nanoid": "^5.0.7",
    "nodemailer": "^6.9.15",
    "pg": "^8.13.0",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "uuid": "^10.0.0"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config",
  "packageManager": "^pnpm@9.11.0"
}