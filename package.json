{
  "name": "properties-backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --watch",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "migrate": "node ace migration:run",
    "db": "echo 'running migration' && node ace migration:run && echo 'seeding database' && node ace db:seed"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#start/*": "./start/*.js",
    "#tests/*": "./tests/*.js",
    "#config/*": "./config/*.js",
    "#types/*": "./types/*.js",
    "#constants/*": "./app/constants/*.js",
    "#helpers/*": ["./app/helpers/*.js"]
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.6.1",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@japa/api-client": "^2.0.3",
    "@japa/assert": "^3.0.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.5.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.12.12",
    "@types/uuid": "^9.0.8",
    "eslint": "^8.57.0",
    "hot-hook": "^0.2.6",
    "pino-pretty": "^11.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.2.1",
    "@adonisjs/core": "^6.9.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/limiter": "^2.3.1",
    "@adonisjs/lucid": "^20.6.0",
    "@adonisjs/mail": "^9.2.1",
    "@adonisjs/redis": "^9.1.0",
    "@vinejs/vine": "^2.1.0",
    "bullmq": "^5.8.2",
    "edge.js": "^6.0.2",
    "luxon": "^3.4.4",
    "mjml": "^4.15.3",
    "mysql2": "^3.10.1",
    "nanoid": "^5.0.7",
    "nodemailer": "^6.9.14",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "uuid": "^10.0.0"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config"
}